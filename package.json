{
  "name": "keypicks-vivu",
  "version": "1.0.0",
  "description": "Flight booking system for KeypicksVIVU",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "build:css": "node_modules\\.bin\\tailwindcss.cmd -i ./ui/css/tailwind-input.css -o ./ui/css/tailwind.css --minify",
    "watch:css": "node_modules\\.bin\\tailwindcss.cmd -i ./ui/css/tailwind-input.css -o ./ui/css/tailwind.css --watch"
  },
  "keywords": [
    "flight",
    "booking",
    "travel",
    "vietnam"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "config/",
      "middleware/",
      "models/",
      "routes/"
    ],
    "ext": "js,json,html,css,ejs"
  }
}
